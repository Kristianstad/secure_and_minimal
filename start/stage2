#!/bin/sh
set -e +a +m +s +i -f

# Passed from stage1:
# -------------------------------------------
# firstrun

. /start/stage2.functions

if [ "$firstrun" == "true" ]
then
   readFirstrunEnvironment
   fixPermissionsForUsrLocalBin
   tryRunStage 3
   writeRestartEnvironment
else
   readRestartEnvironment
   tryRunStage 3
fi
executeCommandAsLinuxUser "$VAR_FINAL_COMMAND"
