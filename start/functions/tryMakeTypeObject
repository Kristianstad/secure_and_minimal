tryMakeTypeObject(){
   local obj="$(removeQuotes $2)"
   local -
   if [ ! -e "$obj" ]
   then
      tryMakeDir "$(/usr/bin/dirname "$obj")" $3
      set +e
      if [ "$1" == "dir" ]
      then
         /bin/mkdir "$obj"
      elif [ "$1" == "file" ]
      then
         >"$obj"
      fi
      set -e
      trySetPermissionsOf "$obj" $3
   fi
}
