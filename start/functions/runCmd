runCmd(){
   eval "/usr/bin/env -i $1"
}

runCmdAsLinuxUser(){
   runCmd "/usr/local/bin/sudo -u $VAR_LINUX_USER $1"
}

runBinCmdAsLinuxUser(){
   /bin/chown $VAR_LINUX_USER /bin
   local -
   set +e
   runCmdAsLinuxUser "$1"
   local exitCode=$?
   set -e
   /bin/chown root /bin
   return $exitCode
}
