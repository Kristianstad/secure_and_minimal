startStage2(){
   local isFirstRun=$1
   readEnvironmentVars
   if [ "$isFirstRun" == "true" ]
   then
      createLinuxUser
      updateSudoersConf
      fixHosts
      makeAllFromList "$(allVars noquote)"
      fixPermissionsForUsrLocalBin
   fi
   tryRunStage 3
   allVars quote > /environment/vars
   if [ "$FINAL_COMMAND_AS_ROOT" == "yes" ]
   then
      execCmd "$VAR_FINAL_COMMAND"
   else
      execCmdAsLinuxUser "$VAR_FINAL_COMMAND"
   fi
}
