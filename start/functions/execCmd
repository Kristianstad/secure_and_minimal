execCmd(){
   local command="$(eval "echo $1")"
   exec /usr/bin/env -i $command
}

execCmdAsLinuxUser(){
   if [ -n "$1" ]
   then
      local command="$(eval "echo $1")"
      execCmd "/usr/local/bin/sudo -u $VAR_LINUX_USER $command"
   else
      echo 'execCmdAsLinuxUser: Nothing to do!'
   fi
}
