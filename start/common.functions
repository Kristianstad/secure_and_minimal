# VAR_*

allVARs(){
   local tmp="$(set | grep "VAR_param")"
   local vars=""
   for var in $tmp
   do
      local varName="$(echo "$var" | /usr/bin/awk -F '=' '{print $1}')"
      eval "local varValue=$var"
      local vars="$vars$(echo "$varName=$varValue")"
   done
   echo $vars
}

executeCommand(){
   exec /usr/bin/env -i $1
}
