secretFromFileOfType(){
   local secret=""
   local nlSecretFile="$(fileWithNlEnding "$1")"
   if [ -s "$nlSecretFile" ]
   then
      read secret < "$nlSecretFile"
      if [ -z "$(eval "echo \$VAR_$2_file_$VAR_LINUX_USER")" ] || [ "$nlSecretFile" != "$1" ]
      then
         tryDelete "$nlSecretFile"
      fi
   fi
   echo "$secret"
}
