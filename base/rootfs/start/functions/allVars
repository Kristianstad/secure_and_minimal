allVars(){
   local IFS=$(echo -en "\n\b")
   local var=""
   local nestedVars=""
   local nestedDelimiter=""
   if [ -z "$1" ]
   then      
      local orderedDelimiter=""
      local orderedVars=""
      for var in $(set | grep -E "^VAR_")
      do
         if echo "$var" | grep -q '$VAR_'
         then
            nestedVars="$nestedVars$nestedDelimiter$var"
            nestedDelimiter=$'\n'
         else
            orderedVars="$orderedVars$orderedDelimiter$var"
            orderedDelimiter=$'\n'
         fi
      done
   else
      local nested=""
      local nvar=""
      local varIsset=""
      for var in $1
      do
         varIsset="true"
         nested="$(echo "$var" | grep -oE '$VAR_[[:alnum:] ]+' | sed 's/$//g')"
         for nvar in $nested
         do
            IFS=""
            if [ "${orderedVars#*$nvar}" == "$orderedVars" ]
            then
               nestedVars="$nestedVars$nestedDelimiter$var"
               nestedDelimiter=$'\n'
               varIsset="false"
               IFS=$(echo -en "\n\b")
               break
            else
               IFS=$(echo -en "\n\b")
            fi
         done
         if [ "$varIsset" == "true" ]
         then
            orderedVars="$orderedVars$orderedDelimiter$var"
            orderedDelimiter2=$'\n'
         fi
      done
   fi
   if [ -z "$nestedVars" ]
   then
      echo "$orderedVars"
   else
      echo "$orderedVars"$'\n'"$(allVars "$nestedVars")"
   fi
}
