setLinuxUserAsOwner(){
   local -
   local file=""
   local dir=""
   set -f
   while read file
   do
      set +f
      find "$(/usr/bin/dirname "$file")" -name "$(/usr/bin/basename "$file")" -user 0 -maxdepth 1 -exec chown 102 "{}" +
   done </environment/linuxuserowned
   set -f
   while read dir
   do
      set +f
      find "$dir" -user 0 -exec chown 102 "{}" +
   done </environment/linuxuserownedrecursive
}
