startStage2(){
   local isFirstRun=$1
   local finalCmdString=""
   readEnvironmentVars
   if [ "$isFirstRun" == "true" ]
   then
      createLinuxUser
      updateSudoersConf
      fixHosts
      makeAllFromList "$(allVars noquote)"
      fixPermissionsForUsrLocalBin
      local ampAppendFinal="true"
   fi
   tryRunStage 3
   allVars quote > /environment/vars
   if [ -z "$finalCmdString" ]
   then
      finalCmdString="$VAR_FINAL_COMMAND &"
   fi
   if [ "$VAR_FINAL_CMD_AS_ROOT" == "yes" ]
   then
      execCmd "$finalCmdString" "final"
   else
      execCmdAsLinuxUser "$finalCmdString" "final"
   fi
}
