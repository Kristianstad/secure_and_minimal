execCmd(){
   if [ -n "$1" ]
   then
      local command="$(eval "echo $1")"
      if [ "$2" == "final" ]
      then
         if [ "$isFirstRun" == "true" ]
         then
            {
               echo "$command &"
               echo "wait"
            } >> /stop/stage1
         fi
         exec /usr/bin/env -i /stop/stage1
      else
         exec /usr/bin/env -i $command
      fi
   else
      echo 'execCmd: Nothing to do!'
   fi
}
