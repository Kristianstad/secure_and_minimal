printClause(){
   local name=$1
   local leftBracket="$(head -1 $2)"
   local rightBracket="$(tail -1 $2)"
   local IFS=$(echo -en "$3")
   local row=""
   local echoStr="$(echo "${name%\#*} $leftBracket")"
   eval "local clauseVar=\"\$VAR_CLAUSE_$name\""
   for row in $clauseVar
   do
      if [ "${row% *}" == "printClause" ]
      then
         echoStr="$echoStr$(eval $row)"
      else
         echoStr="$echoStr$(echo $row)"
      fi
   done
   echo "$echoStr$(echo $rightBracket)"
}
