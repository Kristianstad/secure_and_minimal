containerControl(){
   local isFirstRun=$1
   readEnvironmentVars
   local finalUser=$VAR_LINUX_USER
   find /usr/local/bin/* ! -perm -4000 -exec /bin/chmod ug=rx "{}" +
   if [ "$isFirstRun" == "true" ]
   then
      createLinuxUser
      updateSudoersConf
      fixHosts
      makeAllFromList "$(allVars)"
      local finalCmdString="$(echo "$VAR_FINAL_COMMAND &" | sed 's/\"/\\042/g')"
      sourceDirs /start/stage3
      {
         eval "echo -e \"$finalCmdString\""
         echo 'PID=$!'
         echo 'wait $PID'
         echo 'for sig in 1 2 3 6 9 14 15; do trap - $sig; done'
         echo 'wait $PID'
         echo 'EXIT_STATUS=$?'
      } >> /stop/stage1
      writeEnvironmentVars
   else
      sourceDirs /start/stage3
   fi
   if [ -n "$VAR_STARTUPEXECUTABLES" ]
   then
      local startupExecutables=""
      local executable=""
      for executable in $VAR_STARTUPEXECUTABLES
      do
         startupExecutables="$startupExecutables $(/usr/bin/basename "$executable")"
      done
      startupExecutables="$(trim "$startupExecutables")"
      /usr/bin/env -i /start/disableExecutables "$startupExecutables" &
   fi
   execCmdAsUser /stop/stage1 $finalUser
}
