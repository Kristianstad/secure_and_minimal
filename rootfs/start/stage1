#!/bin/sh
set -e +a +m +s +i -f
. /start/functions/writeEnvironmentVars
. /start/functions/execCmd

if [ ! -e "/environment/vars" ]
then
   isFirstRun="true"
   VAR_START_KEEP_CAPS="cap_chown,cap_fowner,cap_setgid,cap_setuid,cap_setpcap"
   if [ -n "$VAR_KEEP_CAPS" ]
   then
      VAR_START_KEEP_CAPS="$startKeepCaps,$VAR_KEEP_CAPS"
   fi
   writeEnvironmentVars
fi

execCmd "/start/stage2 $isFirstRun"
