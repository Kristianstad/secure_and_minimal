export CHARSET=UTF-8
export PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/start
export PAGER=less
export PS1='\h:\w\$ '
export VAR_LINUX_USER=root
export VAR_ARGON2_PARAMS=-r
export VAR_SALT_FILE=/proc/sys/kernel/hostname
export HISTFILE=/dev/null
umask 022

for script in /etc/profile.d/*.sh ; do
        if [ -r $script ] ; then
                . $script
        fi
done
