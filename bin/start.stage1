#!/bin/sh
set -e +a +m +s +i -f

readonly bin_dir="$(/usr/bin/dirname $0)"

. "$bin_dir/start.common.functions"
. "$bin_dir/start.stage1.functions"

if [ isSomethingToDo ]
then
   if [ isFirstRun ]
   then
      createLinuxUser
      updateSudoersConf
      writeRuntimeEnvironment
   fi
   startStage2
else
   echo "Nothing to do!"
fi
